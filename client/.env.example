PIMLICO_API_KEY=YOUR_API_KEY_HERE
CHAIN=base-sepolia

# Public RPC for reading chain state (any public endpoint for the same chain)
PUBLIC_RPC=https://sepolia.base.org

# Pimlico bundler RPC (v2 path; keep /rpc)
BUNDLER_RPC=https://api.pimlico.io/v2/base-sepolia/rpc?apikey=${PIMLICO_API_KEY}

# EntryPoint (v0.6 is the common default; change if you use v0.7)
ENTRYPOINT=0x5FF137D4b0FDCD49DcA30c7CF57E578a026d2789

# Your deployed contracts (fill these in after deployment)
PAYMASTER=0xYourDeployedPaymasterAddress
TARGET=0xYourMerchantReceiptsOrAnyTargetAddress
